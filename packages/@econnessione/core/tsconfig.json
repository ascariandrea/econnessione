{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "lib": ["DOM", "ESNext"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "outDir": "./lib",
    "baseUrl": "./src",
    "rootDir": "./src",
    "paths": {
      "@Common/*": ["./Common/*"],
      "@http/*": ["./http/*"]
    }
  },
  "include": ["./src"],
  "exclude": ["**/node_modules/**"]
}
