{
  "name": "api",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf build",
    "start": "node -r dotenv/config build/run.js",
    "develop": "DOTENV_CONFIG_PATH=../../.env.local nodemon -r dotenv/config build/run.js",
    "develop:alpha": "DOTENV_CONFIG_PATH=../../.env.alpha nodemon -r dotenv/config build/run.js",
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test:e2e": "jest -c jest.config.js e2e",
    "create-pages": "DOTENV_CONFIG_PATH=../../.env.local ts-node -r dotenv/config -r tsconfig-paths/register ./scripts/create-pages.ts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@remark-embedder/core": "^1.2.1",
    "aws-sdk": "^2.817.0",
    "cors": "2.8.5",
    "debug": "^4.3.1",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "fp-ts": "^2.9.1",
    "front-matter": "4.0.2",
    "gray-matter": "^4.0.2",
    "io-ts": "^2.2.13",
    "io-ts-types": "^0.5.12",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "prettier": "2.2.1",
    "rehype-sanitize": "^4.0.0",
    "ts-endpoint-express": "2.0.0-alpha.6",
    "ts-shared": "^1.0.1",
    "typeorm": "^0.2.29",
    "unified": "9.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "4.17.9",
    "@types/express-jwt": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^12",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "fast-check": "^2.13.0",
    "nodemon": "2.0.6",
    "supertest": "^6.1.3",
    "ts-node": "8.10.2",
    "typeorm-uml": "^1.6.2",
    "typescript": "^4.1.3"
  },
  "_moduleAliases": {
    "@io": "build/io",
    "@entities": "build/entities",
    "@providers": "build/providers",
    "@routes": "build/routes",
    "@utils": "build/utils",
    "@econnessione/core": "../../packages/@econnessione/core/lib",
    "@econnessione/shared": "../../packages/@econnessione/shared/lib"
  },
  "license": "MIT"
}
