{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
